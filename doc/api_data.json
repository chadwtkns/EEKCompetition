[  {    "type": "post",    "url": "/v1/auth/login",    "title": "Log a user in",    "name": "Login",    "group": "Auth",    "examples": [      {        "title": "Example usage:",        "content": "POST /v1/auth/login\n{\n  \"email\": \"email@email.com\",\n  \"password\": \"password\"\n}",        "type": "json"      }    ],    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "<p>Boolean</p> ",            "optional": false,            "field": "success",            "description": "<p>When the request is successful.</p> "          },          {            "group": "Success 200",            "type": "<p>Object</p> ",            "optional": false,            "field": "user",            "description": "<p>The user object.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "user.id",            "description": "<p>The user's unique id.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "user.email",            "description": "<p>The user's email.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "user.role",            "description": "<p>The user's role.</p> "          },          {            "group": "Success 200",            "type": "<p>Date</p> ",            "optional": false,            "field": "user.createdAt",            "description": "<p>The date the user was created.</p> "          },          {            "group": "Success 200",            "type": "<p>Date</p> ",            "optional": false,            "field": "user.updatedAt",            "description": "<p>The date the user was updated.</p> "          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n  \"success\": true,\n  \"user\": {\n    \"id\": \"the-unique-id\",\n    \"email\": \"example@email.com\",\n    \"role\": \"user\",\n    \"createdAt\": Sat Nov 15 2014 08:23:19 GMT-0700 (MST),\n    \"updatedAt\": Sat Nov 15 2014 08:23:19 GMT-0700 (MST)\n  }\n}",          "type": "json"        }      ]    },    "error": {      "examples": [        {          "title": "Error-Response:",          "content": " HTTP/1.1 200 OK\n {\n   \"success\": false,\n   \"error\": \"Incorrect Credentials\"\n }\n\nHTTP/1.1 200 OK\n {\n   \"success\": false,\n   \"error\": \"Max Attempts\"\n }",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "routes/v1/auth-api-docs.js",    "groupTitle": "Auth"  }]